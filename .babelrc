{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-function-bind",
    "transform-decorators-legacy",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": true,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
